{
  "version": 2,
  "buildCommand": "cd Agora-ecommerce-catalog/frontend && npm run build",
  "outputDirectory": "Agora-ecommerce-catalog/frontend/dist",
  "installCommand": "cd Agora-ecommerce-catalog/frontend && npm install --legacy-peer-deps",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}